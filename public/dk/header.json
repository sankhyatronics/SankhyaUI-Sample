[
    {
        "title": "Hovedmenu",
        "Data": {
            "type": "Header",
            "data": {
                "id": "header-main",
                "sticky": true,
                "className": "bg-primary shadow-md max-w-[1920px] mx-auto",
                "mobileBreakpoint": 768,
                "logoHref": "/",
                "logoClassName": "w-[120px]",
                "imageSrc": "logo.svg",
                "altText": "Logo",
                "children": [
                    {
                        "type": "IconButton",
                        "data-position": "far-item",
                        "data": {
                            "id": "util-theme",
                            "onClick": "@action:onThemeChangeClick",
                            "icon": "mdi:theme-light-dark",
                            "className": "bg-primary hover:bg-primary-200 text-primary-950 p-2 rounded-full"
                        }
                    },
                    {
                        "id": "select-language",
                        "type": "Select",
                        "data-position": "far-item",
                        "className": "bg-primary hover:bg-primary-200 text-primary-950 rounded-sm px-4 py-2",
                        "title": "Sprog",
                        "defaultValue": "dk",
                        "hideSelectedLabel": true,
                        "data": {
                            "id": "select-lang",
                            "placeholder": "Vælg sprog",
                            "defaultValue": "dk",
                            "options": [
                                {
                                    "value": "en",
                                    "label": "",
                                    "onClick": "@action:onLanguageChangeToEn",
                                    "icon": "circle-flags:us"
                                },
                                {
                                    "value": "dk",
                                    "label": "",
                                    "onClick": "@action:onLanguageChangeToDk",
                                    "icon": "circle-flags:dk"
                                }
                            ]
                        }
                    },
                    {
                        "type": "MenuItem",
                        "data": {
                            "id": "nav-home",
                            "title": "Forside",
                            "href": "/",
                            "className": "bg-primary hover:bg-primary-200 text-primary-950 rounded-sm px-4 py-2"
                        }
                    },
                    {
                        "type": "Dropdown",
                        "data": {
                            "id": "nav-services",
                            "title": "Services",
                            "className": "bg-primary hover:bg-primary-200 text-primary-950 rounded-sm px-4 py-2",
                            "itemGroupClassName": "bg-primary text-primary-950 p-4"
                        },
                        "children": [
                            {
                                "type": "MenuGrid",
                                "data": {
                                    "id": "services-grid",
                                    "columns": 2,
                                    "className": "rounded-sm shadow-md",
                                    "gridClassName": "gap-6 bg-primary text-primary-950 rounded-sm px-4 py-2 shadow-md w-148"
                                },
                                "children": [
                                    {
                                        "type": "MenuGridItem",
                                        "data": {
                                            "title": "Rådgivning",
                                            "description": "Strategisk rådgivning og teknologiske roadmaps.",
                                            "href": "/services#consultancy",
                                            "icon": "mdi:account-tie-outline",
                                            "className": "bg-primary hover:bg-primary-200 text-primary-950 rounded-sm px-4 py-2"
                                        }
                                    },
                                    {
                                        "type": "MenuGridItem",
                                        "data": {
                                            "title": "Managed Services",
                                            "description": "24/7 support og infrastrukturstyring.",
                                            "href": "/services#managed",
                                            "icon": "mdi:server-network",
                                            "className": "bg-primary hover:bg-primary-200 text-primary-950 rounded-sm px-4 py-2"
                                        }
                                    },
                                    {
                                        "type": "MenuGridItem",
                                        "data": {
                                            "title": "AI Udvikling",
                                            "description": "Skræddersyede Agentic AI løsninger til automatisering.",
                                            "href": "/services#ai",
                                            "icon": "mdi:robot-outline",
                                            "className": "bg-primary hover:bg-primary-200 text-primary-950 rounded-sm px-4 py-2"
                                        }
                                    },
                                    {
                                        "type": "MenuGridItem",
                                        "data": {
                                            "title": "Alle Services",
                                            "description": "Se vores komplette katalog af løsninger.",
                                            "href": "/services",
                                            "icon": "mdi:arrow-right",
                                            "className": "bg-primary-300 hover:bg-primary-200 text-primary-950 rounded-sm px-4 py-2"
                                        }
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "type": "MenuItem",
                        "data": {
                            "id": "nav-contact",
                            "title": "Kontakt os",
                            "href": "/contact-us",
                            "inverted": true,
                            "className": "bg-primary-600 hover:bg-primary-400 text-primary rounded-sm px-4 py-2"
                        }
                    }
                ]
            }
        }
    }
]